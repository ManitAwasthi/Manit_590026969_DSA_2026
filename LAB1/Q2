#include <stdio.h>

int main() {
    int n, rev = 0, temp, rem;

    printf("Enter a number: ");
    scanf("%d", &n);

    temp = n;   // store original number

    while(n > 0) {
        rem = n % 10;
        rev = rev * 10 + rem;
        n = n / 10;
    }

    if(temp == rev)
        printf("Palindrome Number");
    else
        printf("Not a Palindrome Number");

    return 0;
}
